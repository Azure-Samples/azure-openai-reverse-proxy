{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "LoadBalancingPolicy": "RoundRobin",
        "HealthCheck": {
          "Passive": {
            "Enabled": "true",
            "Policy": "RateLimitPolicy",
            "ReactivationPeriod": "00:01:00"
          }
        },
        "Metadata": {
          "RemainingRequestsThreshold": "118",
          "RemainingTokensThreshold": "1000"
        },
        "Destinations": {
          "deployment1": {
            "Address": "https://account-name.openai.azure.com/openai/deployments/deployment-1"
          },
          "deployment2": {
            "Address": "https:///account-name.openai.azure.com/openai/deployments/deployment-2"
          }
        }
      }
    }
  }
}
