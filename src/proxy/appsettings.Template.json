{
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "LoadBalancingPolicy": "RoundRobin",
        "HealthCheck": {
          "Passive": {
            "Enabled": "true",
            "Policy": "AzureOpenAIPassiveHealthCheckPolicy",
            "ReactivationPeriod": "00:01:00"
          }
        },
        "Metadata": {
          "RemainingRequestsThreshold": "118",
          "RemainingTokensThreshold": "1000"
        },
        "Destinations": {
          "deployment1": {
            "Address": "https://gpt-four-open-ai.openai.azure.com/openai/deployments/chat-35-16k"
          },
          "deployment2": {
            "Address": "https://gpt-four-open-ai.openai.azure.com/openai/deployments/chat-35-16k-2"
          }
        }
      }
    }
  }
}
